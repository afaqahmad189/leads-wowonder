		function Wo_SendInvite(user_id, group_id) {
			Wo_progressIconLoader($('#add-' + user_id).find('button'));
			$.get(Wo_Ajax_Requests_File(), {
				f: 'private_group',
				s: 'invite_friends',
				user_id: user_id,
				group_id: group_id
