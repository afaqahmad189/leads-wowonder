			});
		}

		function searchUser(self, type) {
			var name = $(self).val(),
				parent = $(self).parents('form:first'),
				SearchResult = parent.find('.search-list');

			if (name != '') {
				$.ajax({
					url: Wo_Ajax_Requests_File(),
					type: 'GET',
